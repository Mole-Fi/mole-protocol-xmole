{
  "name": "mole-protocol-xmole",
  "version": "0.0.6",
  "license": "ISC",
  "author": "Econiche",
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn run build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "compile": "hardhat typechain --config hardhat.config.ts && yarn build",
    
    "---------------- avalanche testnet mole deploy ---------------":"",
    "deploy:avalanche_testnet:proxy-token:deploy:proxy-token": "hardhat --network avalanche_testnet deploy --no-compile --reset --tags ProxyToken",
    "deploy:avalanche_testnet:xmole:deploy:xmole": "hardhat --network avalanche_testnet deploy --no-compile --reset --tags XMole",
    "deploy:avalanche_testnet:grasshouse:deploy:grasshouse": "hardhat --network avalanche_testnet deploy --no-compile --reset --tags GrassHouse",
    "deploy:avalanche_testnet:grasshouse:deploy:grasshouse-gateway": "hardhat --network avalanche_testnet deploy --no-compile --reset --tags GrassHouseGateway",
    "deploy:avalanche_testnet:mole-feeder:deploy:mole-feeder": "hardhat --network avalanche_testnet deploy --no-compile --reset --tags MoleFeeder",
    
    "---------------- avalanche testnet mole config ---------------":"",
    "config:avalanche_testnet:grasshouse:config:set-enable-checkpoint":"hardhat --network avalanche_testnet deploy --no-compile --reset --tags GrassHouseEnableCheckpoint",
    "config:avalanche_testnet:grasshouse:config:checkpoint":"hardhat --network avalanche_testnet deploy --no-compile --reset --tags GrassHouseCheckpoint",
    "config:avalanche_testnet:grasshouse:config:feed-gr-grasshouse":"hardhat --network avalanche_testnet deploy --no-compile --reset --tags GrassHouseFeed",
    
    "---------------- avalanche testnet mole upgrade ---------------":"",
    "upgrade:avalanche_testnet:xmole:upgrade:xmole-without-timelock":"hardhat --network avalanche_testnet deploy --no-compile --reset --tags UpgradeXMOLEWithoutTimelock",
    "upgrade:avalanche_testnet:grasshouse:upgrade:grasshouse-without-timelock":"hardhat --network avalanche_testnet deploy --no-compile --reset --tags UpgradeGrassHouseWithoutTimelock",

    "---------------- avalanche testnet mole test -----------------":"",
    "test:avalanche_testnet:inspect:grass-house-inspector": "hardhat test --network avalanche_testnet --no-compile ./tests/inspect/GrassHouseInspector.test.ts"
    

},
  "devDependencies": {
    "@ethersproject/bignumber": "^5.0.14",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.4.2",
    "@openzeppelin/contracts-upgradeable": "4.4.2",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.28",
    "chai": "^4.3.0",
    "chai-bn": "^0.2.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.0.10",
    "ethers": "^5.0.31",
    "hardhat": "^2.0.10",
    "hardhat-deploy": "^0.7.0-beta.45",
    "hardhat-typechain": "^0.3.5",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.15",
    "synthetix": "^2.39.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.1",
    "typescript": "^4.1.5"
  },
  "files": [
