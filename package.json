    "follow-redirects": "^1.14.7",
    "glob-parent": "^5.1.2",
    "yargs-parser": "^13.1.2",
    "mem": "^4.0.0",
    "json-schema": "^0.4.0",
    "ansi-regex": "^5.0.1",
    "lodash": "^4.17.21",
    "underscore": "^1.12.1",
    "node-fetch": "^2.6.7",
    "simple-get": "^2.8.2",
    "web3": "^1.7.0"
  }
